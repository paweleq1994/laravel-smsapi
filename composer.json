{
    "name": "vemcogroup/laravel-smsapi",
    "description": "Laravel implementation to easy sent sms via SMSAPI",
    "keywords": ["sms", "laravel", "smsapi"],
    "homepage": "https://github.com/vemcogroup/laravel-smsapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Henrik B Hansen",
            "email": "hbh@vemcount.com",
            "homepage": "https://www.vemcogroup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "smsapi/php-client": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Vemcogroup\\SmsApi\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vemcogroup\\SmsApi\\SmsApiServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "name": "smsapi/php-client",
            "url": "https://github.com/snapshotpl/smsapi-php-client"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
